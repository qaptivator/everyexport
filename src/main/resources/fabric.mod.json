{
  "schemaVersion": 1,
  "id": "everyexport",
  "version": "${version}",
  "name": "EveryExport",
  "description": "exports item and recipe data as json.\n\nmade for enthusiasts, who want to work with actual item data, instead of spending a while trying to find it online.",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/everyexport/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "org.qaptivator.everyexport.client.EveryexportDataGenerator"
    ],
    "client": [
      "org.qaptivator.everyexport.client.EveryexportClient"
    ],
    "main": [
      "org.qaptivator.everyexport.Everyexport"
    ]
  },
  "mixins": [
    "everyexport.mixins.json",
    {
      "config": "everyexport.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
